{
  "name": "@srcpunks/src_ui",
  "description": "@srcpunks/src_ui is a UI component library for @srcpunks apps",
  "private": false,
  "version": "0.0.10",
  "type": "module",
  "module": "./dist/main.js",
  "exports": {
    ".": "./dist/main.js",
    "./style.css": "./dist/assets/main.css"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srcpunks/src_ui.git"
  },
  "homepage": "https://ui.srcpunks.sh/",
  "author": "Xavier Saliniere <bonjour@xaviers.sh>",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9"
  },
  "scripts": {
    "cosmos": "cosmos",
    "cosmos-export": "cosmos-export && touch ./docs/.nojekyll && cp CNAME ./docs/",
    "cosmos-preview": "pnpm dlx http-server ./docs",
    "build": "tsc -p tsconfig.json && vite build --mode lib",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tailwindcss/vite": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.477.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.2",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-cosmos": "^6.2.3",
    "react-cosmos-plugin-vite": "^6.2.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-lib-inject-css": "^2.2.2"
  }
